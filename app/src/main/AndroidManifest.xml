<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grudus.nativeexamshelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.ExamsMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.AddExamActivity" />
        <activity android:name=".activities.SubjectsListActivity" />
        <activity android:name=".activities.AddNewSubjectActivity" />
        <activity android:name=".activities.SingleSubjectExamsActivity" />
        <activity android:name=".activities.UngradedExamsActivity" />
    </application>

</manifest>